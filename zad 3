import random

def print_even_numbers_v2(n):
    
count = 0
 
numbers = []

while count < n:

number = random.randint(-4, 4) * 2

numbers.append(number)

count += 1

print(", ".join(map(str, numbers)))

n = int(input("Podaj liczbÄ™ n: "))

print("Wylosowane liczby parzyste (wersja 2):")

print_even_numbers_v2(n)
