def is_prime(number):
  
if number < 2:
       
return False
  
for i in range(2, int(number ** 0.5) + 1):

if number % i == 0:

return False

return True

def print_prime_numbers(n):

count = 0

number = 2

while count < n:

if is_prime(number):

print(number)

count += 1

number += 1

n = int(input("Podaj liczbÄ™ n: "))

print("Liczby pierwsze:")

print_prime_numbers(n)
