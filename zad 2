import random

def print_even_numbers_v1(n):
    
count = 0

numbers = []

while count < n:

number = random.randint(-8, 8)

if number % 2 == 0:

numbers.append(number)

count += 1

print(", ".join(map(str, numbers)))

n = int(input("Podaj liczbÄ™ n: "))

print("Wylosowane liczby parzyste (wersja 1):")

print_even_numbers_v1(n)
